if (typeof qty === "undefined" || qty === null || qty === "") { return; }  
if (typeof productId === "undefined" || productId === null || productId === "") { return; }  
const product = modelProducts.getData().find((p) => p.id === productId);
let weight = Number(product.grossWeight);

var formattedText = (Number(qty) * weight).toFixed(2); 
return formattedText;


